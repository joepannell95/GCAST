  function parseM3U(text){
    const lines = text.split(/\r?\n/).map(l=>l.trim());
    const out=[];
    for(let i=0;i<lines.length;i++){
      if(lines[i].startsWith('#EXTINF')){
        const name = lines[i].split(',').slice(1).join(',').trim();
        const url = (lines[i+1]||'').trim();
        if(url.startsWith('http')) out.push({name,url});
        i++;
      }
    }
    return out;
  }

  const entries = parseM3U(M3U);
  const player = document.getElementById('player');
  const select = document.getElementById('channelList');
  const search = document.getElementById('search');
  const count = document.getElementById('count');
  const btnFull = document.getElementById('fullscreen');

  function play(url){
    if(window.__hls){window.__hls.destroy(); window.__hls=null}
    if(Hls.isSupported()){
      const hls = new Hls({enableWorker:true,lowLatencyMode:true});
      hls.loadSource(url);
      hls.attachMedia(player);
      window.__hls = hls;
    } else if (player.canPlayType('application/vnd.apple.mpegurl')){
      player.src = url;
    } else {
      alert('HLS not supported in this browser');
    }
    player.play().catch(()=>{});
  }

  function populate(filter=''){
    const flt = entries.filter(e=>e.name.toLowerCase().includes(filter.toLowerCase()));
    select.innerHTML='';
    flt.forEach((e,i)=>{
      const o=document.createElement('option'); o.value=e.url; o.textContent=e.name; select.appendChild(o);
    });
    count.textContent = `${flt.length} / ${entries.length} channels`;
    if(flt.length){ play(flt[0].value||flt[0].url); }
  }

  select.addEventListener('change',()=> play(select.value));
  search.addEventListener('input',()=> populate(search.value));
  btnFull.addEventListener('click',()=>{ if(player.requestFullscreen) player.requestFullscreen(); });

  populate('');
  </script>
